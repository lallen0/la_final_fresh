<p> List of Users </p>

<hr>
<div>
  <div>
    <table border="1">
      <tr>
        <th>
          ID
        </th>

        <th>
          First Name
        </th>

        <th>
          Last Name
        </th>

        <th>
          Level
        </th>

        <th>
          Bio
        </th>

        <th>
          Details
        </th>

        <th>
        </th>
      </tr>

      <% @list_of_users.each do |a_user| %>
      <tr>
        <td>
          <%= a_user.id %>
        </td>

        <td>
          <%= a_user.first_name %>
        </td>

        <td>
          <%= a_user.last_name %>
        </td>

        <td>
          <%= a_user.level %> 
        </td>
        <td>
          <%= a_user.bio %> 
        </td>

        <td>
          <a href="/users/<%= a_user.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<hr>
